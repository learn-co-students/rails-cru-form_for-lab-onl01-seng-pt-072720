<h1>New Song Form</h1>
<%= form_for(@song) do |f| %>
    <label>Song Name:</label>
    <%=f.text_field :name%><br>
    <label>Artist:</label>
    <%=f.text_field :artist_id%><br>
    <%# <%= f.collection_select :artist_id, Artist.order(:name),:id,:name, include_blank: true %><br> %>
    <label>Genre:</label>
    <%=f.text_field :genre_id%><br>
    <%# <%= f.collection_select :genre, Genre.order(:name),:id,:name, include_blank: true %><br> %>

    <%=f.submit%>
<% end %>